<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">

    <title>{% block title %}Welcome!{% endblock %}</title>


    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/bootstrap-theme.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/search_btn.css') }}">
        <link rel="stylesheet" href="{{ asset('css/mainpage_tile.css') }}">
        <link rel="stylesheet" href="{{ asset('css/map.css') }}">
        {#<link rel="stylesheet" href="{{ asset('css/materialize.min.css') }}">#}
        <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

</head>

<body>


<nav class="navbar navbar-inverse">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                    data-target="#navbar-collapse-3"
                    aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/">Final Year Project</a>
        </div>

        <div class="collapse navbar-collapse" id="navbar-collapse-3">
            <ul class="nav navbar-nav">
                {#<li class="active"><a href="/">Home</a></li>#}
                <li><a href="{{ path('map') }}">Map</a></li>
                <li class="dropdown">
                    <a href="{{ path('statistics') }}">Statistics <b
                                class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="{{ path('crimestats') }}">Crime Rate</a></li>
                        <li><a href="{{ path('firestats') }}">Fire Incidents</a></li>
                    </ul>
                </li>
                <li><a href="{{ path('general_info') }}">General Info</a></li>
                <li><a href="{{ path('comp_tool') }}">Comparison Tool</a></li>
                <li><a href="{{ path('faq') }}">Faq</a></li>
                <li><a href="{{ path('about') }}">About</a></li>
            </ul>
        </div><!--/.nav-collapse -->
    </div>
</nav>


<div class="container">
    <div class="row">
        {#<div class="col-md-12">#}
            {% block body %}{% endblock %}
        {#</div>#}
    </div>
</div><!-- /.container -->


{#<div class="fixed-action-btn" style="bottom: 45px; right: 24px;">#}
    {#<a class="btn-floating btn-large waves-effect waves-light red" href="{{ path('comp_tool') }}"><i#}
                {#class="material-icons">compare</i></a>#}
{#</div>#}



{% block javascripts %}
    <script src="{{ asset('js/jquery-3.1.1.min.js') }}"></script>
    <script>window.jQuery || document.write('<script src="../../assets/js/vendor/jquery.min.js"><\/script>')</script>
    <script src="{{ asset('js/bootstrap.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/dropdown.js') }}"></script>
    <script src="{{ asset('js/search_btn.js') }}"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCAvb9PdlmutL-nm7uJSKWJIzcHnXx1JUU&callback=initMap"
            async defer></script>
    <script src="{{ asset('js/materialize.min.js') }}"></script>
    {% block chartJS %}{% endblock %}
{% endblock %}

</body>
</html>
