{% extends 'deprecated_base_categories.html.twig' %}

{% block body %}
    + traffic
{% endblock %}

{% block map %}
    <div id="map"></div>
    <script>
        function initMap() {
            var aston = {lat: 52.487232, lng: -1.8896852};
            var newstrt = {lat: 52.4767888, lng: -1.9005224};
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 14,
                center: aston
            });
            var marker = new google.maps.Marker({
                position: aston,
                map: map
            });
            var marker2 = new google.maps.Marker({
                position: newstrt,
                map: map
            });

            var trafficLayer = new google.maps.TrafficLayer();
            trafficLayer.setMap(map);
        }
    </script>
{% endblock %}